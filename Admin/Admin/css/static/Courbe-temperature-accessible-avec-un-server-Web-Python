<script>
    window.onload = function () {

        function repClick() {
            var id = this.value;
            var inputRep = document.querySelector("#enrIDRep");
            inputRep.value = id;
        }

        var btnRep = document.querySelectorAll(".btnRep");
        for(var i=0; i<btnRep.length; i++) {
            btnRep[i].onclick = repClick;
        }
    };
</script>


