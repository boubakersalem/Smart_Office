        <script type="application/javascript">
            window.onscroll = function() {scrollBar()};
        
            window.onload = function () {
                let btnState = false;
                let btnSeeMore = document.getElementById("btnSeeMore");
        
                function btnSeeMoreClicked() {
                    let seeMoreComms = document.querySelectorAll(".seeMore");
                    for(let i=0; i<seeMoreComms.length; i++) {
                        if(!btnState) {
                            seeMoreComms[i].setAttribute('style', 'display:flex !important');
                        } else {
                            seeMoreComms[i].setAttribute('style', 'display:none !important');
        
                        }
                    }
                    if(!btnState) {
                        btnSeeMore.innerText = "Voir moins";
                        btnState = true;
                    } else {
                        btnSeeMore.innerText = "Voir plus";
                        btnState = false;
                    }
                }
                
                if(btnSeeMore) {
                    btnSeeMore.onclick = btnSeeMoreClicked;
                }
            };
        
            function scrollBar() {
                let winScroll = document.body.scrollTop || document.documentElement.scrollTop;
                let height = document.documentElement.scrollHeight - document.documentElement.clientHeight;
                let scrolled = (winScroll / height) * 100;
                document.getElementById("myBar").style.width = scrolled + "%";
            }
        </script>
